unit ClienteVO;

interface

type
  TClienteVO = class
  private
    FID: Integer;
    FSITUACAO_CLI: Integer;
    FSituacao: String;
    FNOME: String;
    FCPF_CNPJ: String;
    FRG: String;
    FORGAO_RG: String;
    FINSCRICAO_ESTADUAL: String;
    FINSCRICAO_MUNICIPAL: String;
    FDESDE: String;
    FTIPO_PESSOA: String;
    FEXCLUIDO: String;
    FDATA_CADASTRO: String;

  published

    property Id: Integer read FID write FID;
    property IdSituacao: Integer read FSITUACAO_CLI write FSITUACAO_CLI;
    property DescricaoSituacao: String read FSituacao write FSituacao;
    property Nome: String read FNOME write FNOME;
    property CPFOuCNPJ: String read FCPF_CNPJ write FCPF_CNPJ;
    property RG: String read FRG write FRG;
    property OrgaoRG: String read FORGAO_RG write FORGAO_RG;
    property InscricaoEstadual: String read FINSCRICAO_ESTADUAL write FINSCRICAO_ESTADUAL;
    property InscricaoMunicipal: String read FINSCRICAO_MUNICIPAL write FINSCRICAO_MUNICIPAL;
    property Desde: String read FDESDE write FDESDE;
    property PessoaFisicaOuJuridica: String read FTIPO_PESSOA write FTIPO_PESSOA;
    property Excluido: String read FEXCLUIDO write FEXCLUIDO;
    property DataDoCadastro: String read FDATA_CADASTRO write FDATA_CADASTRO;

  end;

implementation

end.
