unit VendaCabecalhoVO;

interface

type
  TVendaCabecalhoVO = class
  private
    FID: Integer;
    FCFOP: Integer;
    FID_ECF_DAV: Integer;
    FID_ECF_PRE_VENDA_CABECALHO: Integer;
    FID_CLIENTE: Integer;
    FID_ECF_FUNCIONARIO: Integer;
    FID_ECF_MOVIMENTO: Integer;
    FCOO: Integer;
    FCCF: Integer;
    FDATA_VENDA: String;
    FHORA_VENDA: String;
    FVALOR_VENDA: Extended;
    FTAXA_DESCONTO: Extended;
    FDESCONTO: Extended;
    FTAXA_ACRESCIMO: Extended;
    FACRESCIMO: Extended;
    FVALOR_FINAL: Extended;
    FVALOR_RECEBIDO: Extended;
    FTROCO: Extended;
    FVALOR_CANCELADO: Extended;
    FSINCRONIZADO: String;
    FTOTAL_PRODUTOS: Extended;
    FTOTAL_DOCUMENTO: Extended;
    FBASE_ICMS: Extended;
    FICMS: Extended;
    FICMS_OUTRAS: Extended;
    FISSQN: Extended;
    FPIS: Extended;
    FCOFINS: Extended;
